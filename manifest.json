{
  "manifest_version": 2,
  "name": "Better nitter",
  "version": "1.0",
  "description": "Better nitter experience",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "content_scripts": [
    {
      "matches": ["https://t.com.sb/*", "https://nitter.snopyta.org/*"],
      "js": ["scripts/profile.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "styles/style.css"
  ]
}
